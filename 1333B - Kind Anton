#include <bits/stdc++.h>
#define ll   long long
#define pb   push_back
#define ld   long double
#define sz   size()
#define lala for(ll i=0;i<n;i++)
#define ed   "\n"
#define ct   cout

using namespace std;

void bahut_tezz()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
}


void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    ll a[n],b[n];
    lala
    {
        cin>>a[i];
    }

    lala
    {
        cin>>b[i];
    }

    ll id=INT_MAX,idx=INT_MAX;

    lala
    {
        if(a[i]==1)
        {
            id=i;
            break;
        }

    }
    lala
    {
        if(a[i]==-1)
        {
            idx=i;
            break;
        }
    }

    for(ll i=0;i<n;i++)
    {
        if(b[i]<a[i] && idx>=i)
        {
            ash=1;
            break;
        }
        else if(b[i]>a[i] && id>=i)
        {
            ash=1;
            break;
        }
    }
    if(ash)
    {
        ct<<"no"<<ed;
    }
    else
    {
        ct<<"YES"<<ed;
    }



}

int main()
{
    bahut_tezz();
    long long t;
    cin>>t;

    while(t--)
    {

        jai_bajrangbali();


    }
















    return 0;
}

