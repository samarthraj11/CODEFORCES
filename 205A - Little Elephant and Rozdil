#include <bits/stdc++.h>
using namespace std;
int main()

{
  int n,b;
  cin>>n;
  int a[n];
  for(int i=0;i<n;i++)
  {
      cin>>a[i];
  }
  int mini=INT_MAX;
  for(int i=0;i<n;i++)
  {
      if(a[i]<mini)
      {
          mini=a[i];
          b=i;
      }
  }
  sort(a,a+n);
  if(a[0]!=a[1])
  {
      cout<<b+1<<endl;
  }
  else
  {
      cout<<"Still Rozdil"<<endl;
  }








    return 0;
}
