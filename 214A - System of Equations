#include <bits/stdc++.h>
using namespace std;
int main()
{
   int k,cnt=0,y,z;
    int n,m;
    cin>>n>>m;
    for(int i=0;i<=n;i++)
    {
        for(int j=0;j<=m;j++)
        {
            if((i*i)+j==n && (j*j)+i==m)
            {
                cnt++;

            }
        }
    }
    cout<<cnt<<endl;

    return 0;
}
