#include <bits/stdc++.h>
using namespace std;

bool prime(int n)
{
    if(n<=1)
    {
        return false;
    }
    for(int i=2;i<n;i++)
    {
        if(n%i==0)
        {
            return false;
        }
    }
    return true;
}



int main()
{
    int n,m;
    cin>>n>>m;
    int cnt=0,res=0;
    for(int i=2;i<=n;i++)
    {
        if(prime(i))
        {
            cnt++;
        }
    }
    for(int j=2;j<=m;j++)
    {
        if(prime(j))
        {
            res++;
        }
    }

    if(res==cnt+1)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }










    return 0;
}





