    int sum=0;
    bool ash=false;
    string s;
    cin>>n>>d;
    ll a[n];
    lala
    {
        cin>>a[i];
        sum=sum+a[i];
    }

    cnt=sum+(n-1)*10;

    if(d>=cnt)
    {
        res=(d-cnt)/5;
