#include <bits/stdc++.h>
#define ll   long long
#define pb   push_back
#define ld   long double
#define sz   size()
#define lala for(ll i=0;i<n;i++)
#define ed   "\n"
#define ct   cout
using namespace std;


void jai_bajrangbali()
{
    ll c,d,e,n,f,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    //cin>>n;
    //ll a[n];
    ll a,b;
    cin>>a>>b>>c;

    sum=a+b+c;

    if(sum<9)
    {
        ct<<"NO"<<ed;
    }

    else
    {
        //x=max(a,max(b,c));
        x=sum/9;


        if(sum%9==0 && a>=x && b>=x && c>=x)
        {
            ct<<"YES"<<ed;
        }

        else
        {
            ct<<"NO"<<ed;
        }
    }

}

int main()
{
    long long t;
    cin>>t;

    while(t--)
    {

        jai_bajrangbali();


    }
















    return 0;
}

