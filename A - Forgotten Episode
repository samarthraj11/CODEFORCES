void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    ll a[n-1];
    for(ll i=0;i<n-1;i++)
    {
        cin>>a[i];
    }

    sort(a,a+n);

    for(ll i=0;i<n;i++)
    {
        if(a[i]!=i+1)
        {
            ct<<i+1<<ed;
            return ;
        }
    }
}

