#include <bits/stdc++.h>
#define ll   long long
#define pb   push_back
#define ld   long double
#define sz   size()
#define lala for(ll i=0;i<n;i++)
#define ed   "\n"
#define ct   cout
#define vi   vector<ll>
#define vc   vector<char>
#define ff   first
#define ss   second

using namespace std;

void bahut_tezz()
{
    std::ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}


void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    //cin>>n;
    //ll a[n];
    cin>>n;
    if(n>0)
    {
        ct<<n<<ed;
        return ;
    }
    n=n*(-1);

    s=to_string(n);
    string p=s;

    for(ll i=0;i<s.sz;i++)
    {
        if(i==(s.sz-2))
        {
            s.erase(s.begin()+i);
            break;
        }
    }

    for(ll i=0;i<p.sz;i++)
    {
        if(i==(p.sz-1))
        {
            p.erase(p.begin()+i);
            break;
        }
    }


    x=stoi(s);
    y=stoi(p);

    ct<<-1*(min(x,y))<<ed;



}

int main()
{
    bahut_tezz();
    long long t;
    //cin>>t;
    t=1;
    while(t--)jai_bajrangbali();

    return 0;
}

