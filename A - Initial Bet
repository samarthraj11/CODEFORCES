void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    //cin>>n;
    ll a[5];
    for(ll i=0;i<5;i++)
    {
        cin>>a[i];
        sum=sum+a[i];
    }
    sort(a,a+5);
    if(sum<5)
    {
        ct<<-1<<ed;
    }
    else if(sum%5 != 0)
    {
        ct<<-1<<ed;
    }
    else
    {
        ct<<sum/5<<ed;
    }
}

