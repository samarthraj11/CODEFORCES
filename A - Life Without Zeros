void bahut_tezz()
{
    std::ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}

ll  binazero(ll n)
{
    string s=to_string(n);
    for(ll i=0;i<s.sz;i++)
    {
        if(s[i]=='0')
        {
            s.erase(s.begin()+i);
            i--;
        }
    }
    ll x=stoi(s);
    return x;
}


void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>x>>y;
    //ll a[n];
    k=x+y;
    if(binazero(k)==binazero(x)+binazero(y))
    {
        ct<< "YES"<<ed;
        return ;
    }
    ct<< "NO"<<ed;


}

