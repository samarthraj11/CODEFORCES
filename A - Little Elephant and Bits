#include <bits/stdc++.h>
#define ll   long long
#define pb   push_back
#define ld   long double
#define sz   size()
#define lala for(ll i=0;i<n;i++)
#define ed   "\n"
#define ct   cout

using namespace std;


int main()
{

    ll c,d,e,n,f,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    //cin>>n;
    //ll a[n];
    cin>>s;
    n=s.sz;
    for(ll i=0;i<n;i++)
    {
        if(s[i]=='0')
        {
            s.erase(s.begin()+i);
            ash=1;
            break;
        }
    }
    if(ash)
    {
        ct<<s<<ed;
    }

    else
    {
        for(ll i=1;i<n;i++)
        {
            ct<<s[i];
        }

    }
























    return 0;
}

