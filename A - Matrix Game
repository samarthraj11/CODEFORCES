#include <bits/stdc++.h>
#define ll   long long
#define pb   push_back
#define ld   long double
#define sz   size()
#define lala for(ll i=0;i<n;i++)
#define ed   "\n"
#define ct   cout
#define vi   vector<ll>

using namespace std;

void bahut_tezz()
{
    std::ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}


void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=0;
    ll sum=0;
    bool ash=false;
    string s;
    ll m;
    cin>>n>>m;
    ll a[n][m];
    for(ll i=0;i<n;i++)
    {
        for(ll j=0;j<m;j++)
        {
            cin>>a[i][j];
        }
    }
    set<int>row,col;

    for(ll i=0;i<n;i++)
    {
        for(ll j=0;j<m;j++)
        {
            if(a[i][j]==1)
            {
                row.insert(i),col.insert(j);
            }
        }
    }

    //ct<<row.sz<< " "<<col.sz<<ed;
    x=n-row.sz;
    y=m-col.sz;
    ll mini =min(x,y);
    if(mini%2==0)
    {
        ct<<"Vivek"<<ed;
    }
    else
    {
        ct<<"Ashish"<<ed;
    }



}

int main()
{
    bahut_tezz();
    long long t;
    cin>>t;
    //t=1;
    while(t--)
    {

        jai_bajrangbali();


    }
















    return 0;
}

