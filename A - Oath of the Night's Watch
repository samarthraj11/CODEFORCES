#include <bits/stdc++.h>
using namespace std;
void jai_bajrangbali()
{
    long long b,c,d,e,n,f,x,y,z,cnt=0,res=0,mx=1;
    cin>>n;
    int a[n];
    bool ash=false;
    string s;

    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }

    sort(a,a+n);
    for(int i=0;i<n;i++)
    {
        if(a[0]==a[i] || a[n-1]==a[i])
        {
            cnt++;
        }
    }



    if(n<=2)
    {
        cout<<0<<endl;
    }
    else
    {
        
        cout<<n-cnt<<endl;
    }



}

int main()
{

    jai_bajrangbali();
















    return 0;
}

