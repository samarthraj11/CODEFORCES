using namespace std;


int main()
{

    ll c,d,e,n,f,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n>>f;
    //ll a[n];
    if(n==1 && f==10)
    {
        ct<<-1<<ed;
        return 0;
    }

    if(f==10)
    {

        x=n-1;
        ct<<1;
        while(x--)
        {
            ct<<0;
        }




    }


    else
    {
        while(n--)
        {
            ct<<f;
        }
    }



















    return 0;
}

