void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    ll a[7];
    for(ll i=0;i<7;i++)
    {
        cin>>a[i];
    }
    k=0;
    while(sum<n)
    {
        if(k==7)
        {
            k=0;
        }
        sum=sum+a[k];
        if(sum>=n)
        {
            break;
        }
        k++;
    }
    ct<<k+1<<ed;


}

