#include <bits/stdc++.h>
#define ll   long long
#define pb   push_back
#define ld   long double
#define sz   size()
#define lala for(ll i=0;i<n;i++)
#define ed   "\n"
#define ct   cout
#define vi   vector<ll>
#define vc   vector<char>
#define ff   first
#define ss   second

using namespace std;

void bahut_tezz()
{
    std::ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}


void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>y>>k>>n;
    //ll a[n];
    if(y>=n)
    {
        ct<<-1<<ed;
        return ;
    }
    if(k>n)
    {
        ct<<-1<<ed;
        return ;
    }
    if(k==n)
    {
        ct<<n-y<<ed;
        return ;

    }

    z=k-(y%k);
    //x=z;
    while(z<=(n-y))
    {
        ct<<z<< " ";
        cnt++;
        z=z+k;
    }
    if(cnt==0)
    {
        ct<<-1<<ed;
    }




}

int main()
{
    bahut_tezz();
    long long t;
    //cin>>t;
    t=1;
    while(t--)jai_bajrangbali();

    return 0;
}

