#include <bits/stdc++.h>
using namespace std;
void jai_bajrangbali()
{
}

int main()
{
    long long t;
    cin>>t;

    while(t--)
    {
        int a,b,c,d,e,n,f,x,y,z,cnt=0,res=0;
        bool ash=false;
        string s;
        cin>>s;
        for(int i=0;i<s.size();i++)
        {
           if(s[i]=='0')
           {
               cnt++;
           }
           else
           {
               res++;
           }
        }


        if(cnt<res && cnt%2!=0)
        {
            cout<<"DA"<<endl;
        }
        else if(res<=cnt && res%2!=0)
        {
            cout<<"DA"<<endl;
        }
        else
        {
            cout<<"NET"<<endl;
        }








    }







    return 0;
}

