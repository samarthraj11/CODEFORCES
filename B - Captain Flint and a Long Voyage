#include <bits/stdc++.h>
#define ll long long
#define ld long double
#define sz size()
#define pb push_back
#define lulu for(int i=0;i<n;i++)
#define ed endl
#define ct cout

using namespace std;


void jai_bajrangbali()
{
    ll c,d,e,n,f,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    //ll a[n];


    if(n%4==0)
    {
        x=n/4;
    }

    else
    {
        x=(n/4)+1;

    }


    f=n-x;
    while(f--)
    {
        ct<<9;
    }

    while(x--)
    {
        ct<<8;
    }

    ct<<ed;


}

int main()
{
    long long t;
    cin>>t;

    while(t--)
    {

        jai_bajrangbali();


    }
















    return 0;
}

