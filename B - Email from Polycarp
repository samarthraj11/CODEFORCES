void bahut_tezz()
{
    std::ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}


void jai_bajrangbali()
{
    ll k,n,m,x,y,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s,p;
    cin>>s>>p;
    //ll a[n];
    ll i=0,j=0;

    while(i<s.sz && j<p.sz)
    {
        if(s[i]==p[j])
        {
            if(s[i]==s[i+1])
            {
                j++;
            }
            else
            {
                 while(s[i]==p[j])
                {
                    j++;
                }


            }
            i++;

        }
        else
        {
            ct<< "NO"<<ed;
            return ;
        }
    }

    if(i != s.sz || j != p.sz)
    {
        ct<< "NO"<<ed;
        return ;
    }

    ct<< "YES"<<ed;





}

int main()
{
    bahut_tezz();
    long long t;
    cin>>t;
    //t=1;
    while(t--)jai_bajrangbali();

    return 0;
}

