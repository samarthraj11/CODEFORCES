void jai_bajrangbali()
{
    ll k,n,m,x,y,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    //ll a[n];
    if(n<1100)
    {
        while(1)
        {
            if(n%11==0)
            {
                ct<< "YES"<<ed;
                return ;
            }

            else
            {
                n=n-111;
            }
            if(n<0)
            {
                break;
            }
        }

        ct<< "NO"<<ed;
        return ;
    }

    ct<< "YES"<<ed;


}

int main()
{
    bahut_tezz();
    long long t;
    cin>>t;
    //t=1;
    while(t--)jai_bajrangbali();

    return 0;
}

