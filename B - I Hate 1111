/*--------------------------------------------------------------------------------------------------------------------------*/


void jai_bajrangbali()
{
    ll n;
    cin>>n;

    ll till = 11*111 - 11 - 111;//mcnugget theorem
    if(n > till)
    {
        ct<< "YES"<<ed;
        return ;
    }
    bool ash = 0;
    for(ll i=0;i<100;i++)
    {
        for(ll j=0;j<100;j++)
        {
            if((11*i) + (111*j) == n)
            {
                ash = 1;
                break;
            }
        }
    }
    ct<<(ash ? "YES" : "NO")<<ed;

    

}

int main()
{
    bahut_tezz();
    long long t=1;
    cin>>t;
    while(t--)jai_bajrangbali();

    return 0;
}
