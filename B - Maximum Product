void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    ll a[n];
    lala
    {
        cin>>a[i];
    }

    sort(a,a+n);

    ll m1,m2,m3;

    //pehle 2 negative ka aur baaki positve;

    m1=a[0]*a[1]*a[n-1]*a[n-2]*a[n-3];
    m2=a[0]*a[1]*a[2]*a[3]*a[n-1];
    m3=a[n-1]*a[n-2]*a[n-3]*a[n-4]*a[n-5];

    ct<<max(m1,max(m2,m3))<<ed;
}

