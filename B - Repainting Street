void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=INT_MIN;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n>>k;
    ll a[n+1];
    for(ll i=1;i<=n;i++)
    {
        cin>>a[i];
    }

    //ct<<cnt<<ed;
    x=INT_MAX;
    for(ll t=1;t<=100;t++)
    {
        sum=0;
            for(ll i=1;i<=n;)
            {
                if(a[i]==t)
                {
                    i++;
                }
            else
                {
                    i=i+k;
                    sum++;
                }


            }
        x=min(x,sum);
    }

    ct<<x<<ed;

}

