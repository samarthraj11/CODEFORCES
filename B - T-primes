bool koi_nhi(ll n)
{
    if(n<2)
    {
        return false;
    }
    for(ll i=2;i*i<=n;i++)
    {
        if(n%i==0)
        {
            return false;
        }
    }

    return true ;
}


void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    //ll a[n];
    z=sqrt(n);


    if(z*z==n && koi_nhi(z))
    {
        ct<< "YES"<<ed;
        return ;
    }

    ct<< "NO"<<ed;

}

int main()
{
    bahut_tezz();
    long long t;
    cin>>t;
    //t=1;
    while(t--)jai_bajrangbali();

    return 0;
}

