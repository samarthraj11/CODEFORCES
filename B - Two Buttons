using namespace std;

void bahut_tezz()
{
    std::ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}


void jai_bajrangbali()
{
    ll k,n,m,x,y,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n>>m;
    //ll a[n];
    if(n>=m)
    {
        ct<<n-m<<ed;
        return ;
    }

   while(1)
   {
       if(m%2==0)
       {
           m=m/2;
       }
       else
       {
           m++;
       }
       //ct<<m<<ed;
       cnt++;
       if(m<=n)
       {
           break;
       }
   }


   ct<<cnt+n-m<<ed;
}

int main()
{
    bahut_tezz();
    long long t;
    //cin>>t;
    t=1;
    while(t--)jai_bajrangbali();

    return 0;
}

