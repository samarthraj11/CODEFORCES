#define ct cout
using namespace std;


void jai_bajrangbali()
{
    ll c,d,e,n,f,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    ll a[n];

    lala
    {
        cin>>a[i];
    }

    for(ll i=0;i<n;i++)
    {
        for(ll j=i+2;j<n;j++)
        {
            if(a[i]==a[j])
            {
                ash=1;
            }
        }
    }

    if(ash)
    {
        ct<<"YES"<<ed;
    }
    else
    {
        ct<<"NO"<<ed;
    }

}

