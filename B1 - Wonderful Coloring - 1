using namespace std;

void bahut_tezz()
{
    std::ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}


void jai_bajrangbali()
{
    ll k,n,m,x,y,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;

    cin>>s;
    //ll a[n];

    map<char,ll> mp;

    for(ll i=0;i<s.sz;i++)
    {
        mp[s[i]]++;
    }

    for(auto i:mp)
    {
        if(i.ss>1)
        {
            cnt++;
        }
        else
        {
            res++;
        }
//        ct<<i.ff<< " "<<i.ss<<ed;

    }


    ct<<cnt+(res/2)<<ed;

}

int main()
{
    bahut_tezz();
    long long t=1;
    cin>>t;
    while(t--)jai_bajrangbali();

    return 0;
}

