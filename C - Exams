void jai_bajrangbali()
{
    ll k,n,m,x,y,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    //ll a[n];

    vector<pll> v;

    lala
    {
        cin>>x>>y;
        v.pb(m_p(x,y));

    }


    sort(v.begin(),v.end());

    ll ans=v[0].ss;
    //ct<<ans<<ed;
    for(ll i=1;i<n;i++)
    {
        if(ans<=v[i].ss)
        {
            ans=v[i].ss;
        }
        else
        {
            ans=v[i].ff;
        }
    }


    ct<<ans<<ed;
}

int main()
{
    bahut_tezz();
    long long t;
    //cin>>t;
    t=1;
    while(t--)jai_bajrangbali();

    return 0;
}

