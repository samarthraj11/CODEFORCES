void jai_bajrangbali()
{
    ll k,n,m,x,y,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    //ll a[n];
    vi v;

    for(ll i=1;i*i<=n;i++)
    {
        if(n%i==0)
        {
            v.pb(i);
        }
    }
//    for(auto i:v)
//    {
//        ct<<i<< " ";
//    }
//    ct<<ed;



    for(ll i=0;i<v.sz;i++)
    {
        if((v[i]*n/v[i])/(__gcd(v[i],n/v[i]))==n)
        {
           y=v[i];
        }
    }

    ct<<y<< " "<<n/y<<ed;
}

int main()
{
    bahut_tezz();
    long long t=1;
    //cin>>t;
    while(t--)jai_bajrangbali();

    return 0;
}

