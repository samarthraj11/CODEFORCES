void jai_bajrangbali()
{
    ll d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    ll mini=INT_MAX;
    cin>>n;
    ll a[n],c[n];
    lala
    {
        cin>>a[i];
        c[i]=a[i];
        mini=min(a[i],mini);
    }

    vi v(n,0);

    for(ll i=0;i<n;i++)
    {
        if(__gcd(a[i],mini)==mini)
        {
            v[i]=1;
        }
    }

    sort(c,c+n);

    for(ll i=0;i<n;i++)
    {
        if(a[i] != c[i] && v[i]==0)
        {
            ct<< "NO"<<ed;
            return ;
        }
    }

    ct<< "YES"<<ed;


}

int main()
{
    bahut_tezz();
    long long t;
    cin>>t;
    //t=1;
    while(t--)jai_bajrangbali();

    return 0;
}

