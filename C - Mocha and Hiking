void jai_bajrangbali()
{
    ll k,n,m,x,y,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cnt=-1;
    cin>>n;
    ll a[n];
    lala
    {
        cin>>a[i];
        if(a[i]==0)
        {
            cnt=i+1;
        }
    }

    if(cnt==-1)
    {
        ct<<n+1<< " ";
        for(ll i=0;i<n;i++)
        {
            ct<<i+1<< " ";
        }

        ct<<ed;
        return ;
    }

    for(ll i=0;i<cnt;i++)
    {
        ct<<i+1<< " ";
    }

    ct<<n+1<< " ";
    for(ll i=cnt;i<n;i++)
    {
        ct<<i+1<< " ";
    }
    ct<<ed;



}

int main()
{
    bahut_tezz();
    long long t=1;
    cin>>t;
    while(t--)jai_bajrangbali();

    return 0;
}

