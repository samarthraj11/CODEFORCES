void jai_bajrangbali()
{
    ll k,n,m,x,y,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    //cin>>n;
    n=3;
    ll a[n];
    lala
    {
        cin>>a[i];
        if(a[i]==1)
        {
            ash=1;
        }
        if(a[i]==2)
        {
            cnt++;
            if(cnt>=2)
            {
                ash=1;
            }
        }

        if(a[i]==3)
        {
            res++;
            if(res>=3)
            {
                ash=1;
            }
        }
    }

    sort(a,a+n);

    if(ash)
    {
        ct<< "YES"<<ed;
        return ;
    }

    if(cnt==1 && a[1]==4 && a[2]==4)
    {
        ct<< "YES"<<ed;
        return ;
    }

    ct<< "NO"<<ed;

}

int main()
{
    bahut_tezz();
    long long t=1;
    //cin>>t;
    while(t--)jai_bajrangbali();

    return 0;
}

