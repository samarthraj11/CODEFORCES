#include <bits/stdc++.h>
#define ll   long long
#define pb   push_back
#define ld   long double
#define sz   size()
#define lala for(ll i=0;i<n;i++)
#define ed   "\n"
#define ct   cout
#define vi   vector<ll>

using namespace std;

void bahut_tezz()
{
    std::ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}


void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    //ll a[n];
    if(n>45)
    {
        ct<<-1<<ed;
        return ;
    }

    if(n<10)
    {
        ct<<n<<ed;
        return ;
    }

    k=9;

    vi v;

    while(n>0)
    {
        if(n>k)
        {
            n=n-k;
            v.pb(k);
            k--;
        }
        else
        {
            v.pb(n);
            break;
        }

    }

    sort(v.begin(),v.end());
    for(auto it:v)
    {
        ct<<it;
    }
    ct<<ed;



}

int main()
{
    bahut_tezz();
    long long t;
    cin>>t;
    //t=1;
    while(t--)jai_bajrangbali();

    return 0;
}

